CREATE TABLE members (
  id INT AUTO_INCREMENT PRIMARY KEY,
  user_id INT NOT NULL,
  nome VARCHAR(100),
  sobrenome VARCHAR(100),
  bairro VARCHAR(100),
  rua VARCHAR(100),
  numero VARCHAR(10),
  casa VARCHAR(10),
  cep VARCHAR(20),
  idade INT,
  sexo ENUM('Masculino', 'Feminino', 'Outro'),
  FOREIGN KEY (user_id) REFERENCES users(id) ON DELETE CASCADE
);
